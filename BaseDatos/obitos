CREATE TABLE `cocheria`.`obituarios` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `nombre` VARCHAR(100) NULL,
  `apellido` VARCHAR(100) NULL,
  `fecha` DATE NULL,
  `imagen` VARCHAR(100) NULL,
  PRIMARY KEY (`id`));